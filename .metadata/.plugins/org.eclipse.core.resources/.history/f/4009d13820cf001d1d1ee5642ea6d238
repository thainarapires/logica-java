public class Array1 {

	public static void main(String[] args) {
		
		String[] cidades = {
				"São Paulo",
				"Campinas",
				"Atibaia",
				"Leme",
				"Vinhedo",
				"Suzano",
				"São Caetano do Sul",
				"Osasco"
			
		};
			System.out.println(cidades.length);
	
	}

}
